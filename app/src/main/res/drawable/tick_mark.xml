<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.1dp" android:color="@color/light_grey"/>
            <size android:width="5dp" android:height="5dp"/>
        </shape>
    </item>
    <item
        android:width="20dp"
        android:height="1dp"
        android:left="5dp"
        android:top="25dp">
        <rotate
            android:fromDegrees="50">
            <shape android:shape="rectangle">

                <solid android:color="@color/light_grey"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="40dp"
        android:height="1dp"
        android:top="20.5dp"
        android:left="16.6dp">
        <rotate
            android:fromDegrees="-41">
            <shape android:shape="rectangle">
                <solid android:color="@color/light_grey"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="50dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</layer-list>